{
  "name": "facelift",
  "description": "Enjoy facebook with a minimalist style.",
  "version": "1.0",
  "permissions": [
    "tabs", "contentSettings", "*://www.facebook.com/*"
  ],
  "content_scripts": [{
    "matches": [
      "*://www.facebook.com/*"
    ],
    "js": ["content.js"],
    "css": ["content.css"],
    "run_at": "document_end"
  }],
  "browser_action": {
    "default_title": "facelift"
  },
  "manifest_version": 2
}
